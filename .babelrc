{
    "presets": [
        [
            "@babel/env",
            {
                "useBuiltIns": "usage",
                "corejs": 3,
                "targets": "> 0.25%, not dead"
            }
        ],
        "@babel/preset-react",
        "@emotion/babel-preset-css-prop"
    ],
    
    "plugins": [
        "emotion",
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread"
    ],
    "sourceMaps": true,
    "retainLines": true,

    "env": {
        "production": {
            "plugins": ["transform-remove-console"]
        }
    }
}